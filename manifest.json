{
  "manifest_version": 2,
  "name": "Test",
  "version": "1.0",

  "description": "Adds a red border and get cookies to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["test_extension.js"]
    }
  ]

}
